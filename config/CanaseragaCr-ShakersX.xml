  <DecodesScript ScriptName="ST">
    <ScriptType>Decodes:goes-self-timed</ScriptType>
    <FormatStatement Label="scan">s(1,a,scan), f(f,a,10d&apos; &apos;)</FormatStatement>
    <FormatStatement Label="stage">
      f(moff,a,5d&apos;#&apos;),x,f(mint-,a,5d&apos; &apos;, 1)&gt;stageVal
    </FormatStatement>
    <FormatStatement Label="stageVal">
      f(s,a,10d&apos; &apos;,1), x, c(&apos;:&apos;,stageVal)&gt;scan
    </FormatStatement>
    <FormatStatement Label="x_vel">
      f(moff,a,5d&apos;#&apos;),x,f(mint-,a,5d&apos; &apos;, 2)&gt;x_velVal
    </FormatStatement>
    <FormatStatement Label="x_velVal">
      f(s,a,10d&apos; &apos;,2), x, c(&apos;:&apos;,x_velVal)&gt;scan
    </FormatStatement>
    <FormatStatement Label="Temp">
      f(moff,a,5d&apos;#&apos;),x,f(mint-,a,5d&apos; &apos;, 3)&gt;TempVal
    </FormatStatement>
    <FormatStatement Label="TempVal">
      f(s,a,10d&apos; &apos;,3), x, c(&apos;:&apos;,TempVal)&gt;scan
    </FormatStatement>
    <FormatStatement Label="BL">f(s,a,6d&apos; &apos;,4)</FormatStatement>
    <FormatStatement Label="error">&gt;scan</FormatStatement>
    <ScriptSensor SensorNumber="1">
