<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<PlatformConfig ConfigName="MRR-FDMT">
  <Description>
    Fresno Dam in Montana - six parameters - elevation, gate 2, gate 4,
    precipitation, air temperature and a throwaway (solar radiation) - 16
    15-minute binary self-timed values
  </Description>
  <EquipmentModel Name="Sutron 8200">
    <EquipmentType>DCP</EquipmentType>
    <Company>Sutron</Company>
    <Model>8200</Model>
    <EquipmentProperty PropertyName="TimeOrder">D</EquipmentProperty>
  </EquipmentModel>
  <ConfigSensor SensorNumber="1">
    <SensorName>elev_res</SensorName>
    <DataType Standard="SHEF-PE" Code="HP"/>
    <RecordingMode>F</RecordingMode>
    <RecordingInterval>00:15</RecordingInterval>
    <TimeOfFirstSample>00:15</TimeOfFirstSample>
    <AbsoluteMin>2374.0</AbsoluteMin>
    <AbsoluteMax>2774.0</AbsoluteMax>
    <ConfigSensorProperty PropertyName="TT">S</ConfigSensorProperty>
  </ConfigSensor>
  <ConfigSensor SensorNumber="2">
    <SensorName>gate2</SensorName>
    <DataType Standard="SHEF-PE" Code="HJ"/>
    <RecordingMode>F</RecordingMode>
    <RecordingInterval>00:15</RecordingInterval>
    <TimeOfFirstSample>00:15</TimeOfFirstSample>
    <ConfigSensorProperty PropertyName="TT">S</ConfigSensorProperty>
  </ConfigSensor>
  <ConfigSensor SensorNumber="3">
    <SensorName>gate4</SensorName>
    <DataType Standard="SHEF-PE" Code="HL"/>
    <RecordingMode>F</RecordingMode>
    <RecordingInterval>00:15</RecordingInterval>
    <TimeOfFirstSample>00:15</TimeOfFirstSample>
    <ConfigSensorProperty PropertyName="TT">S</ConfigSensorProperty>
  </ConfigSensor>
  <ConfigSensor SensorNumber="4">
    <SensorName>cum_prec</SensorName>
    <DataType Standard="SHEF-PE" Code="PC"/>
    <RecordingMode>F</RecordingMode>
    <RecordingInterval>00:15</RecordingInterval>
    <TimeOfFirstSample>00:15</TimeOfFirstSample>
    <ConfigSensorProperty PropertyName="TT">S</ConfigSensorProperty>
  </ConfigSensor>
  <ConfigSensor SensorNumber="5">
    <SensorName>temp_air</SensorName>
    <DataType Standard="SHEF-PE" Code="TB"/>
    <RecordingMode>F</RecordingMode>
    <RecordingInterval>00:15</RecordingInterval>
    <TimeOfFirstSample>00:15</TimeOfFirstSample>
    <ConfigSensorProperty PropertyName="TT">S</ConfigSensorProperty>
  </ConfigSensor>
  <ConfigSensor SensorNumber="6">
    <SensorName>battvolt</SensorName>
    <DataType Standard="SHEF-PE" Code="VB"/>
    <RecordingMode>F</RecordingMode>
    <RecordingInterval>01:00</RecordingInterval>
    <TimeOfFirstSample>00:15</TimeOfFirstSample>
    <ConfigSensorProperty PropertyName="TT">S</ConfigSensorProperty>
  </ConfigSensor>
  <DecodesScript ScriptName="ST">
    <ScriptType>Decodes</ScriptType>
    <FormatStatement Label="st">
      4X,4(F(S,B,3,1),F(S,B,3,2),F(S,B,3,3)),F(S,B,1,6)
    </FormatStatement>
    <ScriptSensor SensorNumber="1">
      <UnitConverter FromUnitsAbbr="raw" ToUnitsAbbr="FT-MSL">
        <Algorithm>linear</Algorithm>
        <A>0.01</A>
        <B>2500.0</B>
        <C>0.0</C>
        <D>0.0</D>
        <E>0.0</E>
        <F>0.0</F>
      </UnitConverter>
    </ScriptSensor>
    <ScriptSensor SensorNumber="2">
      <UnitConverter FromUnitsAbbr="raw" ToUnitsAbbr="FEET">
        <Algorithm>linear</Algorithm>
        <A>0.01</A>
        <B>0.0</B>
        <C>0.0</C>
        <D>0.0</D>
        <E>0.0</E>
        <F>0.0</F>
      </UnitConverter>
    </ScriptSensor>
    <ScriptSensor SensorNumber="3">
      <UnitConverter FromUnitsAbbr="raw" ToUnitsAbbr="FEET">
        <Algorithm>linear</Algorithm>
        <A>0.01</A>
        <B>0.0</B>
        <C>0.0</C>
        <D>0.0</D>
        <E>0.0</E>
        <F>0.0</F>
      </UnitConverter>
    </ScriptSensor>
    <ScriptSensor SensorNumber="4">
      <UnitConverter FromUnitsAbbr="raw" ToUnitsAbbr="INCHES">
        <Algorithm>linear</Algorithm>
        <A>0.01</A>
        <B>0.0</B>
        <C>0.0</C>
        <D>0.0</D>
        <E>0.0</E>
        <F>0.0</F>
      </UnitConverter>
    </ScriptSensor>
    <ScriptSensor SensorNumber="5">
      <UnitConverter FromUnitsAbbr="raw" ToUnitsAbbr="DEGF">
        <Algorithm>linear</Algorithm>
        <A>0.1</A>
        <B>0.0</B>
        <C>0.0</C>
        <D>0.0</D>
        <E>0.0</E>
        <F>0.0</F>
      </UnitConverter>
    </ScriptSensor>
    <ScriptSensor SensorNumber="6">
      <UnitConverter FromUnitsAbbr="raw" ToUnitsAbbr="VOLTS">
        <Algorithm>linear</Algorithm>
        <A>0.234</A>
        <B>10.6</B>
        <C>0.0</C>
        <D>0.0</D>
        <E>0.0</E>
        <F>0.0</F>
      </UnitConverter>
    </ScriptSensor>
  </DecodesScript>
</PlatformConfig>
