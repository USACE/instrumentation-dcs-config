<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<PlatformConfig ConfigName="KEEwithQ">
  <Description>
    River Stage (HG), Skip Bubbler Stage, ADV Flow (Q), skipping 2 other
    parameters (ADV Stage and Index Velocity); RD only transmitting stilling
    well stage and Bubbler stage&#92;non 7/12/19 USGS added a bubbler stage to
    account for low flows since float bottoms out. They added it to the
    transmission so both stages are currently being transmitted.
  </Description>
  <ConfigSensor SensorNumber="1">
    <SensorName>STAGE</SensorName>
    <DataType Standard="CWMS" Code="Stage"/>
    <RecordingMode>F</RecordingMode>
    <RecordingInterval>00:15</RecordingInterval>
    <TimeOfFirstSample>00:00</TimeOfFirstSample>
  </ConfigSensor>
  <ConfigSensor SensorNumber="5">
    <SensorName>BAT</SensorName>
    <DataType Standard="CWMS" Code="Volt"/>
    <RecordingMode>V</RecordingMode>
    <RecordingInterval>00:15</RecordingInterval>
    <TimeOfFirstSample>00:00</TimeOfFirstSample>
    <AbsoluteMin>6.0</AbsoluteMin>
    <AbsoluteMax>25.0</AbsoluteMax>
  </ConfigSensor>
  <ConfigSensor SensorNumber="6">
    <SensorName>FLOW</SensorName>
    <DataType Standard="CWMS" Code="Flow"/>
    <RecordingMode>F</RecordingMode>
    <RecordingInterval>00:15</RecordingInterval>
    <TimeOfFirstSample>00:00</TimeOfFirstSample>
  </ConfigSensor>
  <DecodesScript ScriptName="RD">
    <ScriptType>Decodes</ScriptType>
    <FormatStatement Label="rd">3x,3x,F(s,i,3,1),2x,F(s,i,1,5)</FormatStatement>
    <ScriptSensor SensorNumber="1">
      <UnitConverter FromUnitsAbbr="raw" ToUnitsAbbr="ft">
        <Algorithm>linear</Algorithm>
        <A>0.01</A>
        <B>0.0</B>
        <C>0.0</C>
        <D>0.0</D>
        <E>0.0</E>
        <F>0.0</F>
      </UnitConverter>
    </ScriptSensor>
    <ScriptSensor SensorNumber="5">
      <UnitConverter FromUnitsAbbr="raw" ToUnitsAbbr="volt">
        <Algorithm>linear</Algorithm>
        <A>0.234</A>
        <B>10.6</B>
        <C>0.0</C>
        <D>0.0</D>
        <E>0.0</E>
        <F>0.0</F>
      </UnitConverter>
    </ScriptSensor>
    <ScriptSensor SensorNumber="6">
      <UnitConverter FromUnitsAbbr="raw" ToUnitsAbbr="raw">
        <Algorithm>none</Algorithm>
        <A>0.0</A>
        <B>0.0</B>
        <C>0.0</C>
        <D>0.0</D>
        <E>0.0</E>
        <F>0.0</F>
      </UnitConverter>
    </ScriptSensor>
  </DecodesScript>
  <DecodesScript ScriptName="ST">
    <ScriptType>Decodes</ScriptType>
    <DataOrder>D</DataOrder>
    <FormatStatement Label="st">
      4X,4(3x,F(S,I,3,1),F(S,I,3,6),6x)
    </FormatStatement>
    <ScriptSensor SensorNumber="1">
      <UnitConverter FromUnitsAbbr="raw" ToUnitsAbbr="ft">
        <Algorithm>linear</Algorithm>
        <A>0.01</A>
        <B>0.0</B>
        <C>0.0</C>
        <D>0.0</D>
        <E>0.0</E>
        <F>0.0</F>
      </UnitConverter>
    </ScriptSensor>
    <ScriptSensor SensorNumber="5">
      <UnitConverter FromUnitsAbbr="raw" ToUnitsAbbr="raw">
        <Algorithm>none</Algorithm>
        <A>0.0</A>
        <B>0.0</B>
        <C>0.0</C>
        <D>0.0</D>
        <E>0.0</E>
        <F>0.0</F>
      </UnitConverter>
    </ScriptSensor>
    <ScriptSensor SensorNumber="6">
      <UnitConverter FromUnitsAbbr="raw" ToUnitsAbbr="cfs">
        <Algorithm>linear</Algorithm>
        <A>0.01</A>
        <B>0.0</B>
        <C>0.0</C>
        <D>0.0</D>
        <E>0.0</E>
        <F>0.0</F>
      </UnitConverter>
    </ScriptSensor>
  </DecodesScript>
</PlatformConfig>
