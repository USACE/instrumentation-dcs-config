<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<PlatformConfig ConfigName="MRR-WLSD">
  <Description>
    Keya Paha River at Wewela, SD transmitting 8 -GH(Press), 8-PC, 8- GH(Radar),
    and 1- Batt Volt set on 2/22/2017. USGS owned DCP, data used for inflow cal
    for GAPT
  </Description>
  <EquipmentModel Name="DA-H522">
    <EquipmentType>DCP</EquipmentType>
    <Company>Design Analysis</Company>
    <Model>H522+</Model>
  </EquipmentModel>
  <ConfigSensor SensorNumber="1">
    <SensorName>Stage-Orifice</SensorName>
    <DataType Standard="SHEF-PE" Code="HG"/>
    <DataType Standard="CWMS" Code="Stage-Orifice"/>
    <RecordingMode>F</RecordingMode>
    <RecordingInterval>00:15</RecordingInterval>
    <TimeOfFirstSample>00:00</TimeOfFirstSample>
    <ConfigSensorProperty PropertyName="TT">S</ConfigSensorProperty>
  </ConfigSensor>
  <ConfigSensor SensorNumber="2">
    <SensorName>Precip</SensorName>
    <DataType Standard="SHEF-PE" Code="PC"/>
    <DataType Standard="CWMS" Code="Precip"/>
    <RecordingMode>F</RecordingMode>
    <RecordingInterval>00:15</RecordingInterval>
    <TimeOfFirstSample>00:00</TimeOfFirstSample>
    <ConfigSensorProperty PropertyName="TT">S</ConfigSensorProperty>
  </ConfigSensor>
  <ConfigSensor SensorNumber="3">
    <SensorName>Stage-Radar</SensorName>
    <DataType Standard="SHEF-PE" Code="HR"/>
    <DataType Standard="CWMS" Code="Stage-Radar"/>
    <RecordingMode>F</RecordingMode>
    <RecordingInterval>00:15</RecordingInterval>
    <TimeOfFirstSample>00:00</TimeOfFirstSample>
    <ConfigSensorProperty PropertyName="TT">S</ConfigSensorProperty>
  </ConfigSensor>
  <ConfigSensor SensorNumber="4">
    <SensorName>BattVolt</SensorName>
    <DataType Standard="SHEF-PE" Code="VB"/>
    <RecordingMode>F</RecordingMode>
    <RecordingInterval>01:00</RecordingInterval>
    <TimeOfFirstSample>00:00</TimeOfFirstSample>
    <ConfigSensorProperty PropertyName="TT">S</ConfigSensorProperty>
  </ConfigSensor>
  <DecodesScript ScriptName="ST">
    <ScriptType>Decodes</ScriptType>
    <FormatStatement Label="ST">
      4x,8(F(s,b,3,1)),8(F(S,B,3,3)),8(F(S,B,3,2)),F(S,B,1,4)
    </FormatStatement>
    <ScriptSensor SensorNumber="1">
      <UnitConverter FromUnitsAbbr="raw" ToUnitsAbbr="Feet">
        <Algorithm>linear</Algorithm>
        <A>0.01</A>
        <B>0.0</B>
        <C>0.0</C>
        <D>0.0</D>
        <E>0.0</E>
        <F>0.0</F>
      </UnitConverter>
    </ScriptSensor>
    <ScriptSensor SensorNumber="2">
      <UnitConverter FromUnitsAbbr="raw" ToUnitsAbbr="inches">
        <Algorithm>linear</Algorithm>
        <A>0.01</A>
        <B>0.0</B>
        <C>0.0</C>
        <D>0.0</D>
        <E>0.0</E>
        <F>0.0</F>
      </UnitConverter>
    </ScriptSensor>
    <ScriptSensor SensorNumber="3">
      <UnitConverter FromUnitsAbbr="raw" ToUnitsAbbr="Feet">
        <Algorithm>linear</Algorithm>
        <A>0.01</A>
        <B>0.02</B>
        <C>0.0</C>
        <D>0.0</D>
        <E>0.0</E>
        <F>0.0</F>
      </UnitConverter>
    </ScriptSensor>
    <ScriptSensor SensorNumber="4">
      <UnitConverter FromUnitsAbbr="raw" ToUnitsAbbr="V">
        <Algorithm>linear</Algorithm>
        <A>0.234</A>
        <B>10.6</B>
        <C>0.0</C>
        <D>0.0</D>
        <E>0.0</E>
        <F>0.0</F>
      </UnitConverter>
    </ScriptSensor>
  </DecodesScript>
</PlatformConfig>
