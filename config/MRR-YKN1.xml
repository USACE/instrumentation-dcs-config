<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<PlatformConfig ConfigName="MRR-YKN1">
  <Description>
    6/6/06&#92;tYankton, SD water quality data 6 15-min values TW, WC,DO,Dis Oxy
    Charge, pH,YR
  </Description>
  <EquipmentModel Name="DA-H522">
    <EquipmentType>DCP</EquipmentType>
    <Company>Design Analysis</Company>
    <Model>H522+</Model>
