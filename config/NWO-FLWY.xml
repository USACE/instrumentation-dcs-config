<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<PlatformConfig ConfigName="NWO-FLWY">
  <Description>Laramie River at Fort Laramie, WY</Description>
  <EquipmentModel Name="Sutron 8200">
    <EquipmentType>DCP</EquipmentType>
    <Company>Sutron</Company>
    <Model>8200</Model>
    <EquipmentProperty PropertyName="TimeOrder">D</EquipmentProperty>
  </EquipmentModel>
  <ConfigSensor SensorNumber="1">
    <SensorName>Precip</SensorName>
    <DataType Standard="SHEF-PE" Code="PC"/>
    <DataType Standard="CWMS" Code="Precip"/>
    <RecordingMode>F</RecordingMode>
    <RecordingInterval>00:15</RecordingInterval>
    <TimeOfFirstSample>00:00</TimeOfFirstSample>
    <ConfigSensorProperty PropertyName="TT">S</ConfigSensorProperty>
  </ConfigSensor>
  <ConfigSensor SensorNumber="2">
    <SensorName>Stage</SensorName>
    <DataType Standard="SHEF-PE" Code="HG"/>
    <DataType Standard="CWMS" Code="Stage"/>
    <RecordingMode>F</RecordingMode>
    <RecordingInterval>00:15</RecordingInterval>
    <TimeOfFirstSample>00:00</TimeOfFirstSample>
    <ConfigSensorProperty PropertyName="TT">S</ConfigSensorProperty>
  </ConfigSensor>
  <ConfigSensor SensorNumber="3">
    <SensorName>BattVolt</SensorName>
    <DataType Standard="SHEF-PE" Code="VB"/>
    <DataType Standard="CWMS" Code="Volt"/>
    <RecordingMode>F</RecordingMode>
    <RecordingInterval>01:00</RecordingInterval>
    <TimeOfFirstSample>00:00</TimeOfFirstSample>
    <ConfigSensorProperty PropertyName="TT">S</ConfigSensorProperty>
  </ConfigSensor>
  <ConfigSensor SensorNumber="4">
    <SensorName>Temp-Water</SensorName>
    <DataType Standard="SHEF-PE" Code="TW"/>
    <DataType Standard="CWMS" Code="Temp-Water"/>
    <RecordingMode>F</RecordingMode>
    <RecordingInterval>00:15</RecordingInterval>
    <TimeOfFirstSample>00:00</TimeOfFirstSample>
    <ConfigSensorProperty PropertyName="TT">S</ConfigSensorProperty>
  </ConfigSensor>
  <DecodesScript ScriptName="ST">
    <ScriptType>Decodes</ScriptType>
    <FormatStatement Label="st">
      2X,8(/,F(S,A,5D&apos; &apos;,2),1X,F(S,A,5D&apos;
      &apos;,1)),1X,F(S,A,5D&apos; &apos;,3)
    </FormatStatement>
    <ScriptSensor SensorNumber="1">
      <UnitConverter FromUnitsAbbr="raw" ToUnitsAbbr="INCHES">
        <Algorithm>none</Algorithm>
        <A>0.01</A>
        <B>0.0</B>
        <C>0.0</C>
        <D>0.0</D>
        <E>0.0</E>
        <F>0.0</F>
      </UnitConverter>
    </ScriptSensor>
    <ScriptSensor SensorNumber="2">
      <UnitConverter FromUnitsAbbr="raw" ToUnitsAbbr="FEET">
        <Algorithm>linear</Algorithm>
        <A>1.0</A>
        <B>0.01</B>
        <C>0.0</C>
        <D>0.0</D>
        <E>0.0</E>
        <F>0.0</F>
      </UnitConverter>
    </ScriptSensor>
    <ScriptSensor SensorNumber="3">
      <UnitConverter FromUnitsAbbr="raw" ToUnitsAbbr="VOLTS">
        <Algorithm>none</Algorithm>
        <A>0.234</A>
        <B>10.6</B>
        <C>0.0</C>
        <D>0.0</D>
        <E>0.0</E>
        <F>0.0</F>
      </UnitConverter>
    </ScriptSensor>
    <ScriptSensor SensorNumber="4">
      <UnitConverter FromUnitsAbbr="raw" ToUnitsAbbr="F">
        <Algorithm>none</Algorithm>
        <A>0.018</A>
        <B>32.0</B>
        <C>0.0</C>
        <D>0.0</D>
        <E>0.0</E>
        <F>0.0</F>
      </UnitConverter>
    </ScriptSensor>
  </DecodesScript>
</PlatformConfig>
