<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<PlatformConfig ConfigName="NWO-RWSD">
  <Description>
    Gauge on Redwater River above the city of Belle Fourche, measuring Stage,
    precip and batt volt
  </Description>
  <EquipmentModel Name="SU8200D">
    <EquipmentType>DCP</EquipmentType>
    <Company>SUTRON</Company>
    <Model>SUTRON 8200 SERIES</Model>
    <EquipmentProperty PropertyName="PlatformType">DCP</EquipmentProperty>
    <EquipmentProperty PropertyName="RetrievalMethod">DCP</EquipmentProperty>
    <EquipmentProperty PropertyName="TimeOrder">D</EquipmentProperty>
  </EquipmentModel>
  <ConfigSensor SensorNumber="1">
    <SensorName>Stage</SensorName>
    <DataType Standard="SHEF-PE" Code="HG"/>
    <DataType Standard="CWMS" Code="Stage"/>
    <RecordingMode>F</RecordingMode>
    <RecordingInterval>00:15</RecordingInterval>
    <TimeOfFirstSample>00:00</TimeOfFirstSample>
    <ConfigSensorProperty PropertyName="TT">S</ConfigSensorProperty>
  </ConfigSensor>
  <ConfigSensor SensorNumber="2">
    <SensorName>Precipitation</SensorName>
    <DataType Standard="SHEF-PE" Code="PC"/>
    <DataType Standard="CWMS" Code="Precip"/>
    <RecordingMode>F</RecordingMode>
    <RecordingInterval>00:15</RecordingInterval>
    <TimeOfFirstSample>00:00</TimeOfFirstSample>
    <ConfigSensorProperty PropertyName="TT">S</ConfigSensorProperty>
  </ConfigSensor>
  <ConfigSensor SensorNumber="3">
    <SensorName>BattVolt</SensorName>
    <DataType Standard="SHEF-PE" Code="VB"/>
    <DataType Standard="CWMS" Code="BattVolt"/>
    <RecordingMode>F</RecordingMode>
    <RecordingInterval>00:15</RecordingInterval>
    <TimeOfFirstSample>00:00</TimeOfFirstSample>
    <ConfigSensorProperty PropertyName="TT">S</ConfigSensorProperty>
  </ConfigSensor>
  <DecodesScript ScriptName="ST">
    <ScriptType>Decodes</ScriptType>
    <FormatStatement Label="nolabel_0">
      4x, 8(F(S,B,3,1)),8(F(S,B,3,2)),F(S,B,1,3)
    </FormatStatement>
    <ScriptSensor SensorNumber="1">
      <UnitConverter FromUnitsAbbr="raw" ToUnitsAbbr="ft">
        <Algorithm>linear</Algorithm>
        <A>0.01</A>
        <B>0.0</B>
        <C>0.0</C>
        <D>0.0</D>
        <E>0.0</E>
        <F>0.0</F>
      </UnitConverter>
    </ScriptSensor>
    <ScriptSensor SensorNumber="2">
      <UnitConverter FromUnitsAbbr="raw" ToUnitsAbbr="in">
        <Algorithm>linear</Algorithm>
        <A>0.01</A>
        <B>0.0</B>
        <C>0.0</C>
        <D>0.0</D>
        <E>0.0</E>
        <F>0.0</F>
      </UnitConverter>
    </ScriptSensor>
    <ScriptSensor SensorNumber="3">
      <UnitConverter FromUnitsAbbr="raw" ToUnitsAbbr="Volts">
        <Algorithm>linear</Algorithm>
        <A>0.234</A>
        <B>10.6</B>
        <C>0.0</C>
        <D>0.0</D>
        <E>0.0</E>
        <F>0.0</F>
      </UnitConverter>
    </ScriptSensor>
  </DecodesScript>
</PlatformConfig>
