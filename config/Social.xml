<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<PlatformConfig ConfigName="Social">
  <Description>
    Dam Stage (HK), Social Inlet Stage (HP), Precip (PC), Hamlet Inlet Stage
    (HF), Blackstone River Stage (HG), Bldg Temp (TB), Battery (VB)&#92;nThis
    config file from data transmitted from Social Street Pumping Station
    (includes WFD and Hamlet which are radiod over).
  </Description>
  <ConfigSensor SensorNumber="1">
    <SensorName>Dam Stage</SensorName>
    <DataType Standard="CWMS" Code="Stage-Pool"/>
    <RecordingMode>F</RecordingMode>
    <RecordingInterval>00:15</RecordingInterval>
    <TimeOfFirstSample>00:00</TimeOfFirstSample>
  </ConfigSensor>
  <ConfigSensor SensorNumber="2">
    <SensorName>Social Stage</SensorName>
    <DataType Standard="CWMS" Code="Stage-SOCIAL"/>
