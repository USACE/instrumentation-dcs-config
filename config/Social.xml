    <RecordingInterval>00:15</RecordingInterval>
    <TimeOfFirstSample>00:00</TimeOfFirstSample>
  </ConfigSensor>
  <ConfigSensor SensorNumber="3">
    <SensorName>Precip</SensorName>
    <DataType Standard="CWMS" Code="Precip"/>
    <RecordingMode>F</RecordingMode>
    <RecordingInterval>00:15</RecordingInterval>
    <TimeOfFirstSample>00:00</TimeOfFirstSample>
  </ConfigSensor>
  <ConfigSensor SensorNumber="4">
    <SensorName>Hamlet Stage</SensorName>
    <DataType Standard="CWMS" Code="Stage-HAMLET"/>
    <RecordingMode>F</RecordingMode>
    <RecordingInterval>00:15</RecordingInterval>
    <TimeOfFirstSample>00:00</TimeOfFirstSample>
  </ConfigSensor>
  <ConfigSensor SensorNumber="5">
    <SensorName>Battery</SensorName>
    <DataType Standard="CWMS" Code="Volt-SOCIAL"/>
    <RecordingMode>V</RecordingMode>
    <RecordingInterval>00:15</RecordingInterval>
    <TimeOfFirstSample>00:00</TimeOfFirstSample>
    <AbsoluteMin>6.0</AbsoluteMin>
    <AbsoluteMax>25.0</AbsoluteMax>
  </ConfigSensor>
  <ConfigSensor SensorNumber="6">
    <SensorName>River Stage</SensorName>
    <DataType Standard="CWMS" Code="Stage-RIVER"/>
    <RecordingMode>F</RecordingMode>
    <RecordingInterval>00:15</RecordingInterval>
    <TimeOfFirstSample>00:00</TimeOfFirstSample>
  </ConfigSensor>
  <ConfigSensor SensorNumber="7">
