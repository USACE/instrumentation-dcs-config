<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<PlatformConfig ConfigName="MET">
  <Description>
    Non-Interleaved - PC, RIVER STAGE (HG), SKIP the rest (5 more parameters
    (Accoustic Velocity, X velocity, Y velocity, S, and R), BATTERY VOLTAGE
    (VB))
  </Description>
  <ConfigSensor SensorNumber="1">
    <SensorName>Stage</SensorName>
    <DataType Standard="CWMS" Code="Stage"/>
    <RecordingMode>F</RecordingMode>
    <RecordingInterval>00:15</RecordingInterval>
    <TimeOfFirstSample>00:00</TimeOfFirstSample>
  </ConfigSensor>
  <ConfigSensor SensorNumber="2">
    <SensorName>Precip</SensorName>
    <DataType Standard="CWMS" Code="Precip"/>
    <RecordingMode>F</RecordingMode>
    <RecordingInterval>00:15</RecordingInterval>
    <TimeOfFirstSample>00:00</TimeOfFirstSample>
  </ConfigSensor>
  <DecodesScript ScriptName="RD">
    <ScriptType>Decodes</ScriptType>
    <FormatStatement Label="RD">3x,F(s,i,3,2),F(s,i,3,1)</FormatStatement>
    <ScriptSensor SensorNumber="1">
      <UnitConverter FromUnitsAbbr="raw" ToUnitsAbbr="ft">
        <Algorithm>linear</Algorithm>
        <A>0.01</A>
        <B>0.0</B>
        <C>0.0</C>
        <D>0.0</D>
        <E>0.0</E>
        <F>0.0</F>
      </UnitConverter>
    </ScriptSensor>
    <ScriptSensor SensorNumber="2">
      <UnitConverter FromUnitsAbbr="raw" ToUnitsAbbr="in">
        <Algorithm>linear</Algorithm>
        <A>0.01</A>
        <B>0.0</B>
        <C>0.0</C>
        <D>0.0</D>
        <E>0.0</E>
        <F>0.0</F>
      </UnitConverter>
    </ScriptSensor>
  </DecodesScript>
  <DecodesScript ScriptName="ST">
    <ScriptType>Decodes</ScriptType>
    <DataOrder>D</DataOrder>
    <FormatStatement Label="st">4x,5(F(s,i,3,2)),5(F(s,i,3,1))</FormatStatement>
    <ScriptSensor SensorNumber="1">
      <UnitConverter FromUnitsAbbr="raw" ToUnitsAbbr="ft">
        <Algorithm>linear</Algorithm>
        <A>0.01</A>
        <B>0.0</B>
        <C>0.0</C>
        <D>0.0</D>
        <E>0.0</E>
        <F>0.0</F>
      </UnitConverter>
    </ScriptSensor>
    <ScriptSensor SensorNumber="2">
      <UnitConverter FromUnitsAbbr="raw" ToUnitsAbbr="in">
        <Algorithm>linear</Algorithm>
        <A>0.01</A>
        <B>0.0</B>
        <C>0.0</C>
        <D>0.0</D>
        <E>0.0</E>
        <F>0.0</F>
      </UnitConverter>
    </ScriptSensor>
  </DecodesScript>
</PlatformConfig>
