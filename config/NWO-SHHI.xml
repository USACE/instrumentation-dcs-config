        <A>0.01</A>
        <B>0.0</B>
        <C>0.0</C>
        <D>0.0</D>
        <E>0.0</E>
        <F>0.0</F>
      </UnitConverter>
    </ScriptSensor>
    <ScriptSensor SensorNumber="5">
      <UnitConverter FromUnitsAbbr="raw" ToUnitsAbbr="VOLTS">
        <Algorithm>linear</Algorithm>
        <A>0.234</A>
        <B>10.6</B>
        <C>0.0</C>
        <D>0.0</D>
        <E>0.0</E>
        <F>0.0</F>
      </UnitConverter>
    </ScriptSensor>
  </DecodesScript>
</PlatformConfig>
