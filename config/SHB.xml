<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<PlatformConfig ConfigName="SHB">
  <Description>
    TEMPERATURE (TA), CUMMULATIVE PRECIP (PC), WIND SPEED (US), WIND DIRECTION
    (UD), OCEAN STAGE (HG), HARBOR STAGE (HM), BAR. PRESS. (BP), BATTERY (VB)
  </Description>
  <ConfigSensor SensorNumber="1">
    <SensorName>TEMP</SensorName>
    <DataType Standard="CWMS" Code="Temp-Air"/>
    <RecordingMode>F</RecordingMode>
    <RecordingInterval>00:30</RecordingInterval>
    <TimeOfFirstSample>00:00</TimeOfFirstSample>
  </ConfigSensor>
  <ConfigSensor SensorNumber="2">
