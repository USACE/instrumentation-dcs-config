<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<PlatformConfig ConfigName="HGskipVB">
  <Description>RIVER STAGE (HG), Skip Battery - Selftimed only</Description>
  <ConfigSensor SensorNumber="1">
    <SensorName>River Stage</SensorName>
    <DataType Standard="CWMS" Code="Stage"/>
    <RecordingMode>F</RecordingMode>
    <RecordingInterval>00:15</RecordingInterval>
    <TimeOfFirstSample>00:00</TimeOfFirstSample>
  </ConfigSensor>
  <DecodesScript ScriptName="ST">
    <ScriptType>Decodes</ScriptType>
    <DataOrder>D</DataOrder>
    <FormatStatement Label="st">4X,8(F(S,I,3,1)),8(3X),8(3X)</FormatStatement>
    <ScriptSensor SensorNumber="1">
      <UnitConverter FromUnitsAbbr="raw" ToUnitsAbbr="ft">
        <Algorithm>linear</Algorithm>
        <A>0.01</A>
        <B>0.0</B>
        <C>0.0</C>
        <D>0.0</D>
        <E>0.0</E>
        <F>0.0</F>
      </UnitConverter>
    </ScriptSensor>
  </DecodesScript>
</PlatformConfig>
