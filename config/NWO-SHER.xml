<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<PlatformConfig ConfigName="NWO-SHER">
  <Description>Sherburne Reservoir nr Babb, MT</Description>
  <EquipmentModel Name="Sutron 8200">
    <EquipmentType>DCP</EquipmentType>
    <Company>Sutron</Company>
    <Model>8200</Model>
    <EquipmentProperty PropertyName="TimeOrder">D</EquipmentProperty>
  </EquipmentModel>
  <ConfigSensor SensorNumber="1">
    <SensorName>elev_res</SensorName>
    <DataType Standard="SHEF-PE" Code="HP"/>
    <DataType Standard="CWMS" Code="Elev"/>
    <RecordingMode>F</RecordingMode>
    <RecordingInterval>00:15</RecordingInterval>
    <TimeOfFirstSample>00:00</TimeOfFirstSample>
    <ConfigSensorProperty PropertyName="TT">S</ConfigSensorProperty>
  </ConfigSensor>
  <ConfigSensor SensorNumber="2">
    <SensorName>battvolt</SensorName>
    <DataType Standard="SHEF-PE" Code="VB"/>
    <DataType Standard="CWMS" Code="Volt"/>
    <RecordingMode>F</RecordingMode>
    <RecordingInterval>01:00</RecordingInterval>
    <TimeOfFirstSample>00:00</TimeOfFirstSample>
    <ConfigSensorProperty PropertyName="TT">S</ConfigSensorProperty>
  </ConfigSensor>
  <DecodesScript ScriptName="ST">
    <ScriptType>Decodes</ScriptType>
    <FormatStatement Label="st">
      10X,8(12X,F(S,B,3,1)),F(S,B,1,2)
    </FormatStatement>
    <ScriptSensor SensorNumber="1">
      <UnitConverter FromUnitsAbbr="raw" ToUnitsAbbr="FT">
        <Algorithm>linear</Algorithm>
        <A>0.01</A>
        <B>4700.1</B>
        <C>0.0</C>
        <D>0.0</D>
        <E>0.0</E>
        <F>0.0</F>
      </UnitConverter>
    </ScriptSensor>
    <ScriptSensor SensorNumber="2">
      <UnitConverter FromUnitsAbbr="raw" ToUnitsAbbr="VOLTS">
        <Algorithm>linear</Algorithm>
        <A>0.234</A>
        <B>10.6</B>
        <C>0.0</C>
        <D>0.0</D>
        <E>0.0</E>
        <F>0.0</F>
      </UnitConverter>
    </ScriptSensor>
  </DecodesScript>
</PlatformConfig>
