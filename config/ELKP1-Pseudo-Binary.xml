<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<PlatformConfig ConfigName="ELKP1-Pseudo-Binary">
  <Description>Stage, precip, and battery load in psuedo binary</Description>
  <ConfigSensor SensorNumber="1">
    <SensorName>HG</SensorName>
    <DataType Standard="RSGIS" Code="Stage"/>
    <RecordingMode>F</RecordingMode>
    <RecordingInterval>00:15</RecordingInterval>
    <TimeOfFirstSample>00:00</TimeOfFirstSample>
  </ConfigSensor>
  <ConfigSensor SensorNumber="2">
    <SensorName>PC</SensorName>
    <DataType Standard="RSGIS" Code="Precipitation"/>
    <RecordingMode>F</RecordingMode>
    <RecordingInterval>00:15</RecordingInterval>
    <TimeOfFirstSample>00:00</TimeOfFirstSample>
  </ConfigSensor>
  <ConfigSensor SensorNumber="3">
    <SensorName>BL</SensorName>
    <DataType Standard="RSGIS" Code="Volt"/>
    <RecordingMode>F</RecordingMode>
    <RecordingInterval>00:15</RecordingInterval>
    <TimeOfFirstSample>00:00</TimeOfFirstSample>
  </ConfigSensor>
  <DecodesScript ScriptName="ST">
    <ScriptType>Decodes:goes-self-timed</ScriptType>
    <DataOrder>D</DataOrder>
    <FormatStatement Label="skip">4x,&gt;scan</FormatStatement>
    <FormatStatement Label="scan">
      12F(S,B,3,1),12F(S,B,3,2),F(S,B,1,3)
    </FormatStatement>
    <FormatStatement Label="error"></FormatStatement>
    <ScriptSensor SensorNumber="1">
      <UnitConverter FromUnitsAbbr="raw" ToUnitsAbbr="ft">
        <Algorithm>linear</Algorithm>
        <A>0.01</A>
        <B>0.0</B>
        <C>0.0</C>
        <D>0.0</D>
        <E>0.0</E>
        <F>0.0</F>
      </UnitConverter>
    </ScriptSensor>
    <ScriptSensor SensorNumber="2">
      <UnitConverter FromUnitsAbbr="raw" ToUnitsAbbr="in">
        <Algorithm>linear</Algorithm>
        <A>0.01</A>
        <B>0.0</B>
        <C>0.0</C>
        <D>0.0</D>
        <E>0.0</E>
        <F>0.0</F>
      </UnitConverter>
    </ScriptSensor>
    <ScriptSensor SensorNumber="3">
      <UnitConverter FromUnitsAbbr="raw" ToUnitsAbbr="V">
        <Algorithm>linear</Algorithm>
        <A>1.0</A>
        <B>0.0</B>
        <C>0.0</C>
        <D>0.0</D>
        <E>0.0</E>
        <F>0.0</F>
      </UnitConverter>
    </ScriptSensor>
  </DecodesScript>
</PlatformConfig>
