    <SensorName>BAY STAGE</SensorName>
    <DataType Standard="CWMS" Code="Stage-BAY"/>
    <RecordingMode>F</RecordingMode>
    <RecordingInterval>00:30</RecordingInterval>
    <TimeOfFirstSample>00:00</TimeOfFirstSample>
  </ConfigSensor>
