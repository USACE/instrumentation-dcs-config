    </FormatStatement>
    <FormatStatement Label="BarPres">
      X,F(S,A,8d&apos;,!&apos;,3),&gt;sensorname
    </FormatStatement>
    <FormatStatement Label="Canal">
      X,F(S,A,8d&apos;,!&apos;,10),&gt;sensorname
    </FormatStatement>
