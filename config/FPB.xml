    </FormatStatement>
    <FormatStatement Label="error">&gt;sensorname</FormatStatement>
    <FormatStatement Label="Bay">X,F(S,A,8d&apos;,!&apos;,1),&gt;sensorname</FormatStatement>
    <FormatStatement Label="River">
