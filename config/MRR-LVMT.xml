      <EquipmentType>DCP</EquipmentType>
      <Company>Sutron</Company>
      <Model>8200</Model>
      <EquipmentProperty PropertyName="TimeOrder">D</EquipmentProperty>
    </EquipmentModel>
    <ConfigSensorProperty PropertyName="TT">S</ConfigSensorProperty>
  </ConfigSensor>
  <ConfigSensor SensorNumber="4">
    <SensorName>temp_air</SensorName>
    <DataType Standard="SHEF-PE" Code="TA"/>
    <RecordingMode>F</RecordingMode>
    <RecordingInterval>00:15</RecordingInterval>
    <TimeOfFirstSample>00:00</TimeOfFirstSample>
    <EquipmentModel Name="Sutron 8200">
      <EquipmentType>DCP</EquipmentType>
      <Company>Sutron</Company>
      <Model>8200</Model>
      <EquipmentProperty PropertyName="TimeOrder">D</EquipmentProperty>
    </EquipmentModel>
    <ConfigSensorProperty PropertyName="TT">S</ConfigSensorProperty>
  </ConfigSensor>
  <ConfigSensor SensorNumber="5">
    <SensorName>stage</SensorName>
    <DataType Standard="SHEF-PE" Code="XX"/>
    <RecordingMode>F</RecordingMode>
    <RecordingInterval>00:15</RecordingInterval>
    <TimeOfFirstSample>00:00</TimeOfFirstSample>
    <EquipmentModel Name="Sutron 8200">
      <EquipmentType>DCP</EquipmentType>
      <Company>Sutron</Company>
      <Model>8200</Model>
      <EquipmentProperty PropertyName="TimeOrder">D</EquipmentProperty>
    </EquipmentModel>
    <ConfigSensorProperty PropertyName="TT">S</ConfigSensorProperty>
  </ConfigSensor>
  <ConfigSensor SensorNumber="6">
    <SensorName>stage</SensorName>
    <DataType Standard="SHEF-PE" Code="YY"/>
    <RecordingMode>F</RecordingMode>
    <RecordingInterval>00:15</RecordingInterval>
    <TimeOfFirstSample>00:00</TimeOfFirstSample>
    <EquipmentModel Name="Sutron 8200">
      <EquipmentType>DCP</EquipmentType>
      <Company>Sutron</Company>
      <Model>8200</Model>
      <EquipmentProperty PropertyName="TimeOrder">D</EquipmentProperty>
    </EquipmentModel>
    <ConfigSensorProperty PropertyName="TT">S</ConfigSensorProperty>
  </ConfigSensor>
  <DecodesScript ScriptName="ST">
    <ScriptType>Decodes</ScriptType>
    <FormatStatement Label="st">
      4X,12(F(S,I,3,1)),12(F(S,I,3,3)),12(F(S,I,3,4)),F(S,B,1,2)
    </FormatStatement>
    <ScriptSensor SensorNumber="1">
      <UnitConverter FromUnitsAbbr="raw" ToUnitsAbbr="FEET">
        <Algorithm>linear</Algorithm>
        <A>0.01</A>
        <B>0.0</B>
        <C>0.0</C>
        <D>0.0</D>
        <E>0.0</E>
        <F>0.0</F>
      </UnitConverter>
    </ScriptSensor>
    <ScriptSensor SensorNumber="2">
      <UnitConverter FromUnitsAbbr="raw" ToUnitsAbbr="VOLTS">
        <Algorithm>linear</Algorithm>
        <A>0.234</A>
        <B>10.6</B>
        <C>0.0</C>
        <D>0.0</D>
        <E>0.0</E>
        <F>0.0</F>
      </UnitConverter>
    </ScriptSensor>
    <ScriptSensor SensorNumber="3">
      <UnitConverter FromUnitsAbbr="raw" ToUnitsAbbr="DEGC">
        <Algorithm>linear</Algorithm>
        <A>0.1</A>
        <B>0.0</B>
        <C>0.0</C>
        <D>0.0</D>
        <E>0.0</E>
        <F>0.0</F>
