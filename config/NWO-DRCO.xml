<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<PlatformConfig ConfigName="NWO-DRCO">
  <Description>
    Big Thompson River at Mouth of Canyon Near Drake (BYCANYO)
  </Description>
  <EquipmentModel Name="Sutron 8200">
    <EquipmentType>DCP</EquipmentType>
    <Company>Sutron</Company>
    <Model>8200</Model>
    <EquipmentProperty PropertyName="TimeOrder">D</EquipmentProperty>
  </EquipmentModel>
  <ConfigSensor SensorNumber="1">
    <SensorName>Stage</SensorName>
    <DataType Standard="SHEF-PE" Code="HG"/>
    <DataType Standard="CWMS" Code="Stage"/>
    <RecordingMode>F</RecordingMode>
    <RecordingInterval>00:15</RecordingInterval>
    <TimeOfFirstSample>00:00</TimeOfFirstSample>
    <ConfigSensorProperty PropertyName="TT">S</ConfigSensorProperty>
  </ConfigSensor>
  <ConfigSensor SensorNumber="2">
    <SensorName>Precip</SensorName>
    <DataType Standard="SHEF-PE" Code="PC"/>
    <DataType Standard="CWMS" Code="Precip"/>
    <RecordingMode>F</RecordingMode>
    <RecordingInterval>00:15</RecordingInterval>
    <TimeOfFirstSample>00:00</TimeOfFirstSample>
    <ConfigSensorProperty PropertyName="TT">S</ConfigSensorProperty>
  </ConfigSensor>
  <ConfigSensor SensorNumber="3">
    <SensorName>Temp-Air</SensorName>
    <DataType Standard="SHEF-PE" Code="TA"/>
    <DataType Standard="CWMS" Code="Temp-Air"/>
    <RecordingMode>F</RecordingMode>
    <RecordingInterval>00:15</RecordingInterval>
    <TimeOfFirstSample>00:00</TimeOfFirstSample>
    <ConfigSensorProperty PropertyName="TT">S</ConfigSensorProperty>
  </ConfigSensor>
  <DecodesScript ScriptName="ST">
    <ScriptType>Decodes</ScriptType>
    <FormatStatement Label="ST">
      4X,16(3X),16(3X),16(3X),16(F(S,B,3,3)),16(3X),16(F(S,B,3,2)),16(3X),16(F(S,B,3,1))
    </FormatStatement>
    <ScriptSensor SensorNumber="1">
      <UnitConverter FromUnitsAbbr="raw" ToUnitsAbbr="ft">
        <Algorithm>linear</Algorithm>
        <A>0.01</A>
        <B>0.0</B>
        <C>0.0</C>
        <D>0.0</D>
        <E>0.0</E>
        <F>0.0</F>
      </UnitConverter>
    </ScriptSensor>
    <ScriptSensor SensorNumber="2">
      <UnitConverter FromUnitsAbbr="raw" ToUnitsAbbr="in">
        <Algorithm>linear</Algorithm>
        <A>0.01</A>
        <B>0.0</B>
        <C>0.0</C>
        <D>0.0</D>
        <E>0.0</E>
        <F>0.0</F>
      </UnitConverter>
    </ScriptSensor>
    <ScriptSensor SensorNumber="3">
      <UnitConverter FromUnitsAbbr="raw" ToUnitsAbbr="F">
        <Algorithm>linear</Algorithm>
        <A>0.01</A>
        <B>0.0</B>
        <C>0.0</C>
        <D>0.0</D>
        <E>0.0</E>
        <F>0.0</F>
      </UnitConverter>
    </ScriptSensor>
  </DecodesScript>
</PlatformConfig>
